<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Cannot_start_before_start_time</fullName>
    <active>true</active>
    <errorConditionFormula>Start_Time__c &lt; Work_Order__r.Start_Time__c</errorConditionFormula>
    <errorMessage>Cannot create a Resource Assignment starting before the Work Orders&apos; start time</errorMessage>
</ValidationRule>
