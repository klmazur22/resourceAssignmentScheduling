<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>End_of_the_week</fullName>
    <active>true</active>
    <errorConditionFormula>End_Time__c &gt; Work_Order__r.Week__r.Week_End__c + 3</errorConditionFormula>
    <errorMessage>Can not create assignment, which is not in current week</errorMessage>
</ValidationRule>
