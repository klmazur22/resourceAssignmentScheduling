<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Start_Time_should_be_exact_hour</fullName>
    <active>true</active>
    <errorConditionFormula>VALUE(MID(Text( Start_Time__c ),15,2))&lt;&gt; 0</errorConditionFormula>
    <errorMessage>Start time should be on start hour (For example, 10:00, 16:00, 22:00)</errorMessage>
</ValidationRule>
